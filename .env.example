# Duolingo 账号配置
# 用户名
DUOLINGO_USERNAME=your_duolingo_username
# JWT Token (从浏览器获取: 登录 Duolingo -> F12 -> Application -> Cookies -> jwt_token)
# 或 F12 在控制台使用 document.cookie.match(new RegExp('(^| )jwt_token=([^;]+)'))[0].slice(11) 获取 duolingo_jwt
DUOLINGO_JWT=your_jwt_token_here

# AI Provider Configuration
# 支持的提供商: gemini, openrouter, deepseek, siliconflow, moonshot, zenmux, custom
AI_PROVIDER=openrouter

# API Keys (根据选择的提供商填写对应的 key)
OPENROUTER_API_KEY=your_openrouter_api_key_here
# GEMINI_API_KEY=your_gemini_api_key_here
# DEEPSEEK_API_KEY=your_deepseek_api_key_here
# SILICONFLOW_API_KEY=your_siliconflow_api_key_here
# MOONSHOT_API_KEY=your_moonshot_api_key_here
# ZENMUX_API_KEY=your_zenmux_api_key_here
# CUSTOM_API_KEY=your_custom_api_key_here

# Model Configuration
AI_MODEL=mistralai/ministral-3b

# Custom Base URL (仅当使用 custom 提供商时需要)
AI_BASE_URL=

# API 访问控制令牌 (可选，用于保护 API 不被非法调用)
API_SECRET_TOKEN=
